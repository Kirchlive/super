{
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/Kirchlive"
    },
    {
      "pattern": "^https://github.com/opencode"
    },
    {
      "pattern": "^https://modelcontextprotocol.io"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "file://{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept": "text/html"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 301, 302, 303, 307, 308]
}